<!-- #region Modal Clientes -->
<div class="modal fade" data-backdrop="static" data-keyboard="false" id="modalContacto" aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Cliente</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <div class="form-row">
                            <div class="form-group col-xs-12 col-md-12">
                                <small>Nombre</small>
                                <input type="text" class="form-control reqGuardar" id="txtNombre" />
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-xs-12 col-md-12">
                                <small>Apellido Paterno</small>
                                <input type="text" class="form-control reqGuardar" id="txtApellidoPaterno" />
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-xs-12 col-md-12">
                                <small>Apellido Materno</small>
                                <input type="text" class="form-control reqGuardar" id="txtApellidoMaterno" />
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-xs-12 col-md-12">
                                <small>Teléfono</small>
                                <input type="text" class="form-control reqGuardar" id="txtTelefono" />
                            </div>
                        </div>

                        <div class="form-row">
                            <div class="form-group col-xs-12 col-md-12">
                                <small>Dirección</small>
                                <textarea class="form-control reqGuardar" id="txtDireccionContacto"></textarea>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-xs-12 col-md-12">
                                <small>Puesto</small>
                                <input type="text" class="form-control reqGuardar" id="txtPuesto" />
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                <button type="button" class="btn btn-primary" id="btnGuardarContactoDetalle">Cuadar</button>
            </div>
        </div>
    </div>
</div>
<!-- #endregion -->